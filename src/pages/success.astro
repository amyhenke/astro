---
import Layout from "../layouts/Layout.astro"

const seo = {
    title: "Thank you! | Amy Henke",
    description: "Web Developer Portfolio",
    image: "../socialbanner.jpg",
    canonicalURL: "https://amyhenke.com/success",
}
const { bodyClass = "success" } = Astro.props
---

<Layout {seo} {bodyClass}>
    <main id="main" class="typography flex flex-col min-h-screen">
        <div
            class="pagetitlesection flex justify-center flex-col px-8 py-10 max-w-screen-md"
        >
            <p class="pretext">Contact</p>
            <h1>Thank you!</h1>
            <h2>Your form submission has been received.</h2>
            <a href="/" class="backarrow"
                ><i class="fa-solid fa-left-long"></i> Back Home</a
            >
        </div>
    </main>
</Layout>
